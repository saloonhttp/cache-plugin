{
    "name": "sammyjo20/saloon-cache-plugin",
    "description": "Official plugin for caching Saloon responses",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Saloon\\CachePlugin\\": "src/",
            "Saloon\\CachePlugin\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sammyjo20",
            "email": "29132017+Sammyjo20@users.noreply.github.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "sammyjo20/saloon": "2.0.0-beta1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.8",
        "pestphp/pest": "^1.21.2",
        "spatie/ray": "^1.34.2",
        "league/flysystem": "^3.0",
        "orchestra/testbench": "^7.0",
        "psr/simple-cache": "^3.0"
    },
    "scripts": {
        "test": [
            "./vendor/bin/pest"
        ],
        "fix-code": [
            "./vendor/bin/php-cs-fixer fix"
        ]
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
